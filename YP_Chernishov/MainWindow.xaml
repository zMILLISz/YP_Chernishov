<Window x:Class="YP_Chernishov.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YP_Chernishov"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition Height="202*"/>
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Grid.Row="1" Source="Pages/AuthPage.xaml" Navigated="MainFrame_Navigated" NavigationUIVisibility="Hidden"/>
        <TextBlock x:Name="Data" Margin="622,10,27,10" Grid.Row="2" TextAlignment="Right" FontSize="20"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="10,0,0,0" Grid.Row="0" VerticalAlignment="Center" Height="35" Width="127" Click="Back_Click"/>
    </Grid>
</Window>
